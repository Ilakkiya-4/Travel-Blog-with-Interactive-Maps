{
    "name": "turismo",
    "version": "1.0.0",
    "description": "Turismo responsive website built using HTML5, CSS3, and JavaScript. The complete layout of the page is built using grid layout and flexbox with some cool animations.",
    "main": "index.html",
    "scripts": {
      "watch:sass": "sass --watch sass/main.scss:css/style.css",
      "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
      "parcel": "parcel build index.html -d dist",
      "build": "npm run prefix:css && npm run parcel",
      "dev": "parcel index.html"
    },
    "author": "Manoranjan",
    "license": "ISC",
    "engines": {
      "node": "20.x"
    },
    "devDependencies": {
      "autoprefixer": "^10.2.5",
      "sass": "^1.56.0"
  // Replaced node-sass with sass
      "parcel": "^2.0.0-beta.2",
      "parcel-bundler": "^1.12.5",
      "postcss": "^8.2.13",
      "postcss-cli": "^8.3.1"
    },
    "dependencies": {
      "aos": "^3.0.0-beta.6"
    }
  }
  